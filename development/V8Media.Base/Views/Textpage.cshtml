@using V8Media.Framework.Models.PublishedContent
@inherits UmbracoTemplatePage<Textpage>	
@{
    Layout = "Base.cshtml";
}
<main>
    <div class="row">
        <div class="column small-12">
            <h1 class="title">@Model.Content.Header</h1>
            <div class="body-text">
                @Html.Raw(Model.Content.BodyText)
            </div>
        </div>
    </div>
</main>